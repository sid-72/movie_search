<%- include("./partials/header")%>
<!--Main Page-->





<div class="container">
    <h1><%=movie.Title%></h1>
    <!--Card-->
    <div class="row">
  <div class="card flex-row flex-wrap">
    <div class="card-header border-0">
      <img src="<%=movie.Poster%>" alt="Poster" />
    </div>
  </div>

    <%if(movie.Rated=='R'){%>
      <div class="card" style="width: 18rem;">
        <img class="card-img-top" src="data:image/png;base64,">
        
      </div>
      <%}%>

<div class="card" style="width: 18rem;">
  <div class="card-body">
    <h5 class="card-title">Ratings</h5>
    
      <%movie['Ratings'].forEach(function(rating){%>
        <p class="card-text">
          <ul>
      <li><%=rating.Source%>-<%=rating.Value%></li>


      </ul>


      <%})%>
    </p>

    
   
  </div>
</div>

</div>

</div>
  
   

    <div class="container">
      <div class="card-block px-2">
      <p class="card-text"><b>Plot:</b> <%=movie.Plot%></p>
      <p class="card-text"><b>Actors:</b> <%=movie.Actors%></p>
      </div>
      <div class="w-100"></div>
      <div class="card-footer w-100 text-muted"><strong>Box Office: <%=movie.BoxOffice%></strong></div>
    </div>    


<%- include("./partials/footer")%>
